{
    "Logging": {
        "IncludeScopes": false,
        "Debug": {
            "LogLevel": {
                "Default": "Warning"
            }
        },
        "Console": {
            "LogLevel": {
                "Default": "Warning"
            }
        }
    },
    "options": {
        "VfsProvider": ["FileSystemVirtualFiles", "GoogleCloudVirtualFiles", "AzureBlobVirtualFiles", "S3VirtualFiles", "R2VirtualFiles"],
        "SpeechProvider": ["GoogleCloudSpeechToText", "AwsSpeechToText", "AzureSpeechToText", "WhisperLocalSpeechToText", "WhisperApiSpeechToText"],
        "TypeChatProvider": ["NodeTypeChat", "KernelTypeChat"]
    },
    "VfsProvider": "FileSystemVirtualFiles",
    "SpeechProvider": "WhisperApiSpeechToText",
    "TypeChatProvider": "NodeTypeChat",
    "GoogleCloudConfig": {
        "Project": "servicestackdemo",
        "Location": "global",
        "Bucket": "servicestack-typechat"
    },
    "AwsConfig": {
        "Bucket": "servicestack-typechat"
    },
    "R2Config": {
        "Bucket": "servicestack-typechat"
    },
    "AzureConfig": {
        "ContainerName": "servicestack-typechat"
    },
    "AppConfig": {
        "CoffeeShop": {
            "GptPath": "gpt/coffeeshop",
            "RecognizerId": "coffeeshop-recognizer",
            "PhraseSetId": "coffeeshop-phrases"
        }
    },
    "AllowedHosts": "*"
}
